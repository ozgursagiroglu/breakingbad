    "abort-controller": "^3.0.0",
    "babel-jest": "^26.6.3",
    "cross-fetch": "^3.1.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "msw": "^0.44.2",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mock__/ImageMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-navigation|react-native-linear-gradient)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ]
  }
}
